{
    "name": "answers",
    "version": "0.0.1",
    "description": "A community driven question and answer forum.",
    "long_description": "Answers is a collaboratively edited question and answer app. It is modelled after stackoverflow.\n\nUsers can post questions, answers and comments. ",
    "url": "https://github.com/ryanramage/question-flow",
    "categories": ["forum"],
    "attachments": ["static", "index.html"],
    "icons": {
        "16": "static/icons/wiki_icon_16.png",
        "48": "static/icons/wiki_icon_48.png",
        "96": "static/icons/wiki_icon_96.png",
        "128": "static/icons/wiki_icon_128.png"
    },
    "promo_images": {
        "small": "static/images/promo_small.png"
    },
    "screenshots": [
        "static/images/screenshot1.png",
        "static/images/screenshot2.png"
    ],

    "modules": "lib",
    "load": "lib/app",
    "less": {
        "compress": true,
        "compile": ["static/css/main.less"],
        "remove_from_attachments": true
    },
    "handlebars": {
        "all_partials": true,
        "templates": "templates"
    },
    "dependencies": {
        "attachments": null,
        "modules": null,
        "less-precompiler": null,
        "bootstrap": null,
        "properties": null,
        "underscore": null,
        "handlebars": null,
        "handlebars-helpers": "0.0.2",
        "git-info" : null,
        "db" : null,
        "sanitize" : null,
        "events" : null,
        "session" : null,
        "gravatar" : null,
        "stew" : null,
        "jquery" : null
    }
}
